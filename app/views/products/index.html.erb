<div class="container">
	<div class="row">
		<div class="col-xs-12">
			<h1 class="text-centerx">Productos</h1>
			<table class="table">
				<tr>
					<th>Id</th>
					<th>Name</th>
					<th>URL</th>
					<th>Description</th>
				</tr>
				<% @products.each do |thing| %>
					<tr>
						<td><%= thing.id %></td>
						<td><%= thing.name %></td>
						<td><%= thing.url %></td>
						<td><%= thing.description %></td>
					</tr>
				<% end %>
			</table>
		</div>
	</div>
</div>


